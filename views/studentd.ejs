        <% const currentDate=new Date(); %> 
        <% const options={ weekday: 'long'}; %> 
        <% var currentDay=currentDate.toLocaleDateString('en-US',options).substring(0,3).toLowerCase(); %> 
        <% if((currentDay ==="sun")||(currentDay==="sat")) { %>
          <% currentDay = "mon"; %>
        <% } %>

        <% Object.keys(timetable[currentDay]).forEach((timeSlot)=> { %> 
        <% if (timetable[currentDay][timeSlot]==="BREAK" ) { %>
        <tr>
          <td class="text-center"><%= timeSlot %></td>
          <td class="text-center"><em>BREAK</em></td>
        </tr>
        <% } else { %>
        <tr>
          <td class="text-center"><%= timeSlot %></td>
          <td class="text-center">
            <% if(timetable[currentDay][timeSlot].SUBJECT){ %> <%=
            timetable[currentDay][timeSlot].SUBJECT %>
            <br />
            <%= timetable[currentDay][timeSlot].TEACHER %> <% } else { %> <%
            Object.keys(timetable[currentDay][timeSlot]).forEach((batches)=> {
            %> <%= batches %> <%= timetable[currentDay][timeSlot][batches] %>
            <br />
            <% }) %> <% } %>
          </td>
        </tr>
        <% }; %> <% }) %>
        
